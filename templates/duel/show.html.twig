{% extends 'base.html.twig' %}

{% block title %}Duel{% endblock %}

{% block body %}
    <h1>Duel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ duel.id }}</td>
            </tr>
            <tr>
                <th>Winst</th>
                <td>{{ duel.winst ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Ronde</th>
                <td>{{ duel.ronde }}</td>
            </tr>
            <tr>
                <th>Score</th>
                <td>{{ duel.score }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('duel_index') }}">back to list</a>

    <a href="{{ path('duel_edit', {'id': duel.id}) }}">edit</a>

    {{ include('duel/_delete_form.html.twig') }}
{% endblock %}
